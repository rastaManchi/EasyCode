{
    "Создание БД": "CREATE DATABASE <название>",
    "Создание таблицы": "CREATE TABLE <Название таблицы>(ID int ...)",
    "Удалить таблицу": "DROP TABLE <название таблицы>",
    "Удалить БД": "DROP DATABASE <название>",
    "Изменить таблицу": "ALTER TABLE <Название таблицы> <ADD/MODIFY COLUMN/DROP COLUMN> ...",
    "Использовать БД": "use <название>"
}